program VariblesDemo;
uses crt;
var
    a   : integer = 23;
    b   : real = 456.12;
    c   : char = 'h';
    d   : string = 'this is a string';
begin
    writeln('a = ', a); //  23
    writeln('b = ', b); //  456.12
    writeln('c = ', c); //  'h'
    writeln('d = ', d); //  'this is a string'
end.